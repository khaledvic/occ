


@interface System
{
}
-(int) readint;
-(char) readchar;
-(float) readfloat;
-(NSString) readstring;


-(void) print:(int)parameter;
-(void) print:(float)parameter;

-(void) print:(NSString)parameter;
-(void) print:(char)parameter;





